{
	"name": "srbcodes",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"dev:wrangler": "wrangler dev",
		"build": "next build",
		"build:cf": "next build && open-next build",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
		"db:generate": "drizzle-kit generate:sqlite --schema=src/db/schema.js --out=src/db/migrations",
		"db:migrate": "wrangler d1 migrations apply srbcodes-db --local",
		"db:migrate:prod": "wrangler d1 migrations apply srbcodes-db --remote"
	},
	"dependencies": {
		"@cloudflare/d1": "^1.4.1",
		"@opennextjs/cloudflare": "^1.0.3",
		"@paralleldrive/cuid2": "^2.2.2",
		"@radix-ui/react-scroll-area": "^1.2.8",
		"@radix-ui/react-separator": "^1.1.6",
		"@radix-ui/react-slot": "^1.2.2",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.20.13",
		"drizzle-orm": "^0.29.3",
		"framer-motion": "^12.12.1",
		"gsap": "^3.13.0",
		"lucide-react": "^0.511.0",
		"next": "15.3.2",
		"next-themes": "^0.4.6",
		"notion-client": "^7.3.0",
		"ogl": "^1.0.11",
		"react": "^19.0.0",
		"react-awesome-button": "^7.0.5",
		"react-dom": "^19.0.0",
		"react-notion-x": "^7.3.0",
		"server-only": "^0.0.1",
		"sonner": "^2.0.3",
		"tailwind-merge": "^3.3.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@tailwindcss/typography": "^0.5.16",
		"@types/node": "22.15.19",
		"@types/react": "19.1.4",
		"eslint": "^9",
		"eslint-config-next": "15.3.2",
		"tailwindcss": "^4",
		"tailwindcss-animate": "^1.0.7",
		"tw-animate-css": "^1.3.0",
		"wrangler": "^4.15.2"
	}
}
